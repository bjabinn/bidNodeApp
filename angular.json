{
    "version": 1,
    "projects": {
        "showcase": {
            "projectType": "application",
            "schematics": {
                "@nrwl/angular:component": {
                    "style": "scss"
                }
            },
            "root": "apps/showcase",
            "sourceRoot": "apps/showcase/src",
            "prefix": "bid",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/apps/showcase",
                        "index": "apps/showcase/src/index.html",
                        "main": "apps/showcase/src/main.ts",
                        "polyfills": "apps/showcase/src/polyfills.ts",
                        "tsConfig": "apps/showcase/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/showcase/src/favicon.ico",
                            "apps/showcase/src/assets"
                        ],
                        "styles": [
                            {
                                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
                            },
                            "./libs/bid-assets/src/lib/showcase-theme.scss",
                            "./libs/bid-assets/src/lib/front-modules.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/showcase/src/environments/environment.ts",
                                    "with": "apps/showcase/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "extractCss": true,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "showcase:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "showcase:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "showcase:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/showcase/tsconfig.app.json",
                            "apps/showcase/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!apps/showcase/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/showcase/jest.config.js",
                        "tsConfig": "apps/showcase/tsconfig.spec.json",
                        "passWithNoTests": true,
                        "setupFile": "apps/showcase/src/test-setup.ts",
                        "styles": [
                            {
                                "input": "node_modules/@progress/kendo-theme-default/dist/all.css"
                            }
                        ]
                    }
                }
            }
        },
        "showcase-e2e": {
            "root": "apps/showcase-e2e",
