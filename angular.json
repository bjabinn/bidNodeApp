        "pocnest-middleware": {
            "root": "apps/pocnest/middleware",
            "sourceRoot": "apps/pocnest/middleware/src",
            "projectType": "application",
            "prefix": "pocnest-middleware",
            "schematics": {},
            "architect": {
                "build": {
                    "builder": "@nrwl/node:build",
                    "options": {
                        "outputPath": "dist/apps/pocnest/middleware",
                        "main": "apps/pocnest/middleware/src/main.ts",
                        "tsConfig": "apps/pocnest/middleware/tsconfig.app.json",
                        "assets": ["apps/pocnest/middleware/src/assets"]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "extractLicenses": true,
                            "inspect": false,
                            "fileReplacements": [
                                {
                                    "replace": "apps/pocnest/middleware/src/environments/environment.ts",
                                    "with": "apps/pocnest/middleware/src/environments/environment.prod.ts"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@nrwl/node:execute",
                    "options": {
                        "buildTarget": "pocnest-middleware:build"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "apps/pocnest/middleware/tsconfig.app.json",
                            "apps/pocnest/middleware/tsconfig.spec.json"
                        ],
                        "exclude": [
                            "**/node_modules/**",
                            "!apps/pocnest/middleware/**"
                        ]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "apps/pocnest/middleware/jest.config.js",
                        "tsConfig": "apps/pocnest/middleware/tsconfig.spec.json",
                        "passWithNoTests": true
                    }
                }
            }
        }
    },
    "cli": {
        "defaultCollection": "@nrwl/angular",
        "analytics": false
    },
    "schematics": {
        "@nrwl/angular:application": {
            "unitTestRunner": "jest",
            "e2eTestRunner": "cypress"
        },
        "@nrwl/angular:library": {
            "unitTestRunner": "jest"
        }
    },
    "defaultProject": "preparation-process"
}
