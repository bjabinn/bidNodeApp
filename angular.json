{
    "version": 1,
    "projects": {
        "bid-ui": {
            "projectType": "library",
            "root": "libs/bid-ui",
            "sourceRoot": "libs/bid-ui/src",
            "prefix": "bid",
            "architect": {
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "libs/bid-ui/tsconfig.lib.json",
                            "libs/bid-ui/tsconfig.spec.json"
                        ],
                        "exclude": ["**/node_modules/**", "!libs/bid-ui/**"]
                    }
                },
                "test": {
                    "builder": "@nrwl/jest:jest",
                    "options": {
                        "jestConfig": "libs/bid-ui/jest.config.js",
                        "tsConfig": "libs/bid-ui/tsconfig.spec.json",
                        "passWithNoTests": true,
                        "setupFile": "libs/bid-ui/src/test-setup.ts"
                    }
                }
            },
            "schematics": {
                "@nrwl/angular:component": {
                    "styleext": "scss"
                }
            }
        },
